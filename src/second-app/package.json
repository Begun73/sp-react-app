{
  "name": "second-app",
  "version": "1.0.0",
  "homepage": "/SiteAssets/pages/Second",
  "scripts": {
    "start": "npm run copy-shareable & set PORT=3012 && react-scripts start",
    "copy-shareable": "Xcopy /E /I /y ..\\shareable src\\shareable 2>null",
    "move-js": "Xcopy /E /I /y build\\*.js ..\\..\\remote\\SiteAssets\\pages\\Second",
    "move-css": "Xcopy /E /I /y build\\*.css ..\\..\\remote\\SiteAssets\\pages\\Second",
    "move-html": "Xcopy /E /I /y build\\*.html ..\\..\\remote\\SiteAssets\\pages\\Second",
    "move-map": "Xcopy /E /I /y build\\*.map ..\\..\\remote\\SiteAssets\\pages\\Second",
    "build": "npm run copy-shareable & react-scripts build --prefix src/second-app && RMDIR /S /Q ..\\..\\build\\SiteAssets\\pages\\Second 2>null & npm run move-js && npm run move-css && npm run move-html && npm run move-map"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8082"
}
